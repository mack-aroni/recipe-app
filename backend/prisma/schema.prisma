generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id              String               @id
  name            String?
  favoriteRecipes FavoriteRecipes[]
  
}

model FavoriteRecipes {
  id       Int    @id @default(autoincrement())
  userId   String
  recipeId Int
  user     User   @relation(fields: [userId], references: [id])
}
